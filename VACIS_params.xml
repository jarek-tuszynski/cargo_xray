<?xml version="1.0" encoding="utf-8"?>
<param>
   <!-- calibration of the input files -->
   <air_transmission>5.29e+04</air_transmission>  <!-- mean transmission values for x-ray photons in open air, as stored in raw input files -->
   <!-- table mapping -log(inImage/air_transmission)/0.766 to actual inches of steel -->
   <CalibrationTable>[0,0;1,0.9695;2,1.7934;3,2.4434;4,2.9097;5,3.2282;6,3.4443;7,3.5672;8,3.6331;9,3.6695;10,3.6896;11,3.6996;12,3.7043]</CalibrationTable>

   <!-- output image parameters  -->
   <max_thickness>12</max_thickness>  <!-- Penetration limit of the system measured in inches of steel. In the final image maximum value will be equivanet to this number -->

   <!-- tiling parameters  -->
   <tile_scale>1</tile_scale>         <!-- Should tiles be scaled? -->
   <maxTileSize>640</maxTileSize>     <!-- Final tile size -->
   <minTileSize>64</minTileSize>      <!-- ignore tiles smaller than this number -->
   <tileOverlap>0.2</tileOverlap>     <!-- fraction of the tile overlap -->
</param>